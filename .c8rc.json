{
  "all": true,
  "check-coverage": true,
  "lines": 90,
  "functions": 90,
  "branches": 90,
  "statements": 90,
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/**/*.test.ts",
    "src/**/*.spec.ts",
    "test/**/*",
    "dist/**/*",
    "**/*.d.ts"
  ],
  "reporter": [
    "text",
    "text-summary",
    "html",
    "lcov"
  ],
  "reports-dir": "coverage"
}
