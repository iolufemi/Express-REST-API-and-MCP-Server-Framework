{
  "name": "api-template",
  "version": "0.7.0",
  "description": "A template for creating APIs Fast",
  "type": "module",
  "main": "dist/app.js",
  "scripts": {
    "dev": "npx tsx watch src/app.ts",
    "build": "rm -rf dist && npx tsc -p tsconfig.build.json",
    "start": "node dist/app.js",
    "test": "node test/run.mjs",
    "test:watch": "node test/run.mjs --watch",
    "test:coverage": "node scripts/run-coverage-with-timeout.mjs",
    "test:save": "npm test 2>&1 | tee test-results.txt",
    "test:simple": "node test/run.mjs --no-config --node-option import=tsx --node-option import=test/setup.ts test/simple.test.ts --reporter spec --timeout 20000",
    "lint": "npx eslint src/**/*.ts",
    "type-check": "npx tsc --noEmit",
    "type-check:test": "npx tsc -p tsconfig.test.json --noEmit",
    "clock": "npx tsx src/services/queue/clock.ts",
    "workers": "npx tsx src/services/queue/workers.ts",
    "gulp": "npx gulp --gulpfile gulpfile.cjs",
    "generate": "npx gulp --gulpfile gulpfile.cjs service"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iolufemi/api-template.git"
  },
  "keywords": [
    "REST",
    "API"
  ],
  "author": "Olufemi Olanipekun <iolufemi@ymail.com>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/iolufemi/api-template/issues"
  },
  "homepage": "https://github.com/iolufemi/api-template#readme",
  "overrides": {
    "minimist": "^1.2.6",
    "lodash": "^4.17.23",
    "fsevents": "^2.3.3"
  },
  "devDependencies": {
    "@types/chai": "^4.3.15",
    "@types/cors": "^2.8.17",
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.21",
    "@types/mocha": "^10.0.6",
    "@types/node": "^22.10.2",
    "@types/randomstring": "^1.1.9",
    "@types/sinon": "^17.0.3",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "c8": "^9.1.0",
    "chai": "^4.3.10",
    "chai-as-promised": "^8.0.0",
    "conventional-github-releaser": "^1.1.12",
    "eslint": "^9.15.0",
    "gulp": "^4.0.2",
    "gulp-bump": "^2.7.0",
    "gulp-conventional-changelog": "^1.1.3",
    "gulp-eslint": "^6.0.0",
    "gulp-git": "^2.10.1",
    "gulp-mocha": "^7.0.2",
    "gulp-nodemon": "^2.5.0",
    "gulp-todo": "^5.4.0",
    "minimist": "^1.2.6",
    "mocha": "^10.7.3",
